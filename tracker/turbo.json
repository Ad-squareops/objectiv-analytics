{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/main",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"]
    },
    "test": {
      "dependsOn": [],
      "outputs": [],
      "inputs": ["jest.config.js", "src/**/*.tsx", "src/**/*.ts", "tests/**/*.ts", "tests/**/*.tsx"]
    },
    "test:ci": {
      "dependsOn": [],
      "outputs": [],
      "inputs": ["jest.config.js", "src/**/*.tsx", "src/**/*.ts", "tests/**/*.ts", "tests/**/*.tsx"]
    },
    "test:coverage": {
      "dependsOn": [],
      "outputs": ["coverage/**"],
      "inputs": ["jest.config.js", "src/**/*.tsx", "src/**/*.ts", "tests/**/*.ts", "tests/**/*.tsx"]
    },
    "prettify": {
      "outputs": [],
      "inputs": ["src/**/*.tsx", "src/**/*.ts", "tests/**/*.ts", "tests/**/*.tsx"]
    },
    "depcheck": {
      "outputs": [],
      "inputs": ["package.json", "src/**/*.tsx", "src/**/*.ts", "tests/**/*.ts", "tests/**/*.tsx"]
    },
    "tsc": {
      "outputs": [],
      "inputs": ["src/**/*.tsx", "src/**/*.ts", "tests/**/*.ts", "tests/**/*.tsx"]
    },
    "generate": {
      "outputs": ["generate/**"],
      "inputs": ["../schema/base_schema.json5", "generateErrorMessages.ts", "generator.js"]
    },
    "pre-publish": {
      "dependsOn": ["depcheck", "tsc", "build", "test"],
      "outputs": []
    }
  }
}
