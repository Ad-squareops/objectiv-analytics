{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/main",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"]
    },
    "test": {
      "dependsOn": [],
      "outputs": [],
      "inputs": ["src/**/*.tsx", "src/**/*.ts", "tests/**/*.ts", "tests/**/*.tsx"]
    },
    "test:ci": {
      "dependsOn": [],
      "outputs": [],
      "inputs": ["src/**/*.tsx", "src/**/*.ts", "tests/**/*.ts", "tests/**/*.tsx"]
    },
    "test:coverage": {
      "dependsOn": [],
      "outputs": ["coverage/**"],
      "inputs": ["src/**/*.tsx", "src/**/*.ts", "tests/**/*.ts", "tests/**/*.tsx"]
    },
    "prettify": {
      "outputs": []
    },
    "depcheck": {
      "outputs": []
    },
    "tsc": {
      "outputs": []
    },
    "deploy": {
      "dependsOn": ["depcheck", "tsc", "build", "test"],
      "outputs": []
    }
  }
}
