{
  "name": "event_list",
  "version": {"base_schema": "0.0.1"},
  "description": "This is the Objectiv event list schema",
  "properties": {
    "events": {
      "type": "array",
      "items": "AbstractEvent",
      "description": "List/array of events. See the base_schema for the definition of an event"
    },
    "transport_time": {
      "type": "int",
      "description": "Timestamp when the list of events was sent by the tracker"
    }
  }
}
